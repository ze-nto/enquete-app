{
	"compilerOptions": {
			"target": "ES6",
			"lib": [
					"DOM",
					"DOM.Iterable",
					"ESNext"
			],
			"esModuleInterop": true,
			"module": "esnext",
			"moduleResolution": "Node",
			"jsx": "react",
			"rootDir": "src",
			"experimentalDecorators":true,
			"baseUrl": "src",
			"paths": {
				"@/*": ["*"]
			},
			"allowJs": true,
			"resolveJsonModule": true,
	},
	"include": [
			"src"
	],
	"exclude": [
		"src/main/test/cypress"
	]
}